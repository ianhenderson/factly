{
  "name": "factly",
  "version": "0.0.0",
  "description": "factly ======",
  "main": "server.js",
  "engines": {
    "node": "4.3.0"
  },
  "scripts": {
    "test:unit": "jasmine",
    "test:e2e": "node_modules/.bin/protractor spec/protractor.conf.js || true",
    "test": "npm run test:unit & npm run test:e2e",
    "build:angular": "node ./build.js",
    "build:es6": "browserify -t babelify -e public/es6/src/entry.js -o public/es6/dist/main.js",
    "build:es6-dev": "npm run build:es6 -- -d",
    "build:dev": "npm run build:angular & npm run build:es6-dev",
    "build": "npm run build:angular & npm run build:es6",
    "dev": "nodemon",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ianhenderson/factly.git"
  },
  "author": "Ian Henderson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianhenderson/factly/issues"
  },
  "dependencies": {
    "angular": "~1.3.10",
    "angular-animate": "~1.3.18",
    "angular-aria": "~1.3.18",
    "angular-material": "0.8.3",
    "angular-sanitize": "^1.4.7",
    "angular-ui-router": "~0.2.13",
    "babelify": "^7.0.2",
    "bcryptjs": "2.2.0",
    "bluebird": "2.9.24",
    "body-parser": "1.6.6",
    "browserify": "^12.0.1",
    "compression": "1.5.2",
    "cookie-parser": "1.3.4",
    "express": "4.8.6",
    "ng-annotate": "1.0.2",
    "page": "^1.6.4",
    "partialify": "^3.1.5",
    "shelljs": "^0.5.3",
    "sqlite3": "3.0.8",
    "uglify-js": "2.4.24"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "chromedriver": "^2.19.0",
    "eslint": "^1.8.0",
    "nodemon": "1.7.3",
    "protractor": "^2.5.1",
    "supertest": "^1.1.0"
  }
}
